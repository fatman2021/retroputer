{
  "name": "retroputer",
  "version": "2.0.0",
  "description": "retroputer",
  "esm": {
    "cjs": true,
    "mode": "all",
    "sourceMap": true
  },
  "main": "index.js",
  "browser": "index.js",
  "dependencies": {
    "cli": "^1.0.1",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "c8": "^4.1.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint-plugin-ava": "^6.0.0",
    "esm": "^3.2.22",
    "mocha": "^6.1.4"
  },
  "scripts": {
    "test": "ava",
    "watch:test": "ava --watch",
    "cover": "c8 --reporter html ava",
    "coveralls": "c8 ava; c8 report --reporter html --reporter=text-lcov | coveralls",
    "cover:view": "open ./coverage/index.html",
    "lint": "eslint src/**/*.js test/**/*.js bin/**/*.js --ignore-pattern bundle.js",
    "build:asm-grammar": "pegjs -o src/grammars/asm.js src/grammars/asm.pegjs",
    "build:kite-grammar": "pegjs -o src/grammars/kite.js src/grammars/kite.pegjs",
    "build:grammars": "npm run build:asm-grammar && npm run build:kite-grammar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerrishotts/retroputer.git"
  },
  "author": "Kerri Shotts <kerrishotts@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kerrishotts/retroputer/issues"
  },
  "homepage": "https://github.com/kerrishotts/retroputer#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "verbose": false,
    "files": [
      "test/**/*.js",
      "!test/tmp/**/*"
    ]
  }
}
